{
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "tabWidth": 2,
  "useTabs": true,
  "printWidth": 80,
  "bracketSameLine": false,
  "bracketSpacing": false,
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "proseWrap": "always",
  "insertPragma": false,
  "requirePragma": false,
  "embeddedLanguageFormatting": "auto"
}
